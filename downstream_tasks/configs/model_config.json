{
    "model_type": "diffusion_cond",
    "sample_size": 221184,
    "sample_rate": 44100,
    "audio_channels": 2,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "iterate_batch": true,
            "config": {
                "encoder": {
                    "type": "oobleck",
                    "requires_grad": false,
                    "config": {
                        "in_channels": 2,
                        "channels": 128,
                        "c_mults": [1, 2, 4, 8, 16],
                        "strides": [2, 4, 4, 8, 8],
                        "latent_dim": 128,
                        "use_snake": true
                    }
                },
                "decoder": {
                    "type": "oobleck",
                    "config": {
                        "out_channels": 2,
                        "channels": 128,
                        "c_mults": [1, 2, 4, 8, 16],
                        "strides": [2, 4, 4, 8, 8],
                        "latent_dim": 64,
                        "use_snake": true,
                        "final_tanh": false
                    }
                },
                "bottleneck": {
                    "type": "vae"
                },
                "latent_dim": 64,
                "downsampling_ratio": 2048,
                "io_channels": 2
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 128
                    }
                },
                {
                    "id": "seconds_start",
                    "type": "number",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                },
                {
                    "id": "seconds_total",
                    "type": "number",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                },
                {
                    "id": "azimuth",
                    "type": "number",
                    "config": {
                        "min_val": 0,
                        "max_val": 359
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "cross_attention_cond_ids": ["prompt", "seconds_start", "seconds_total", "azimuth"],
            "global_cond_ids": ["seconds_start", "seconds_total", "azimuth"],
            "type": "dit",
            "config": {
                "io_channels": 64,
                "embed_dim": 1536,
                "depth": 24,
                "num_heads": 24,
                "cond_token_dim": 768,
                "global_cond_dim": 2304,
                "project_cond_tokens": true,
                "transformer_type": "continuous_transformer"
            }
        },
        "io_channels": 64
    },
    "training": {
        "use_ema": false,
        "log_loss_info": false,
        "cfg_dropout_prob": 0.8,
        "pre_encoded": true,
        "optimizer_configs": {
            "diffusion": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 5e-5,
                        "betas": [0.9, 0.999],
                        "weight_decay": 1e-3
                    }
                },
                "scheduler": {
                    "type": "CosineAnnealingWarmRestarts",
                    "config": {
                        "T_0": 8000,
                        "T_mult": 2,
                        "eta_min": 5e-7
                    }
                }
            }
        },
        "demo": {
            "demo_every": 2000,
            "demo_steps": 250,
            "num_demos": 4,
            "demo_cond": [
                {"prompt": "environment sound", "seconds_start": 0, "seconds_total": 5, "azimuth": 0},
                {"prompt": "environment sound", "seconds_start": 0, "seconds_total": 5, "azimuth": 45},
                {"prompt": "environment sound", "seconds_start": 0, "seconds_total": 5, "azimuth": 90},
                {"prompt": "environment sound", "seconds_start": 0, "seconds_total": 5, "azimuth": 315}
            ],
            "demo_cfg_scales": [3, 6, 9]
        }
    }
}